{
  "name": "@junhokims/eslint-plugin",
  "version": "1.0.0",
  "description": "junhoKim's ESLint Plugins",
  "author": {
    "name": "junhoKims",
    "email": "kraksin12@gmail.com"
  },
  "type": "commonjs",
