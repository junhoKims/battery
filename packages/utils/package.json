{
  "name": "@junhokims/utils",
  "version": "0.0.1",
  "description": "Various Utils",
  "author": {
    "name": "junhoKims",
    "email": "kraksin12@gmail.com"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "esm"
  ],
  "main": "src/index.ts",
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist esm && tsc -p tsconfig.build.json && rollup -c rollup.config.js",
    "test": "jest -c jest.config.js && tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@junhokims/jest-config": "workspace:^",
    "@junhokims/rollup-config": "workspace:^0.0.1",
    "@types/jest": "^29.5.9",
    "jest": "^29.7.0",
    "rollup": "^4.5.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    },
    "import": "./esm/index.mjs",
    "main": "./dist/index.js",
    "module": "./esm/index.mjs",
    "types": "./dist/index.d.ts"
  }
}
