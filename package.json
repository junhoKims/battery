{
  "name": "battery",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "junhoKims",
    "email": "kraksin12@gmail.com"
  },
  "keywords": [],
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "build": "yarn ultra -r build",
    "check:lint": "yarn eslint --quiet docs/src packages/*/src --ext .ts,.tsx,.js,.jsx",
    "check:lint-stage": "yarn eslint --quiet",
    "check:prettier": "yarn prettier --write --log-level silent docs/src/**/*.{ts,tsx,js,jsx} packages/*/src/**/*.{ts,tsx,js,jsx}",
    "check:prettier-stage": "yarn prettier --write --log-level silent",
    "check:type": "tsc --noEmit",
    "test": "yarn ultra -r test --silent",
    "test:silent": "yarn ultra -r test:silent --silent",
    "pre-commit": "lint-staged --no-stash"
  },
  "ultra": {
    "concurrent": [
      "build",
      "test",
      "test:silent"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.3",
    "typescript": "5.2.2",
    "ultra-runner": "^3.10.5"
  },
  "resolutions": {
    "typescript": "5.2.2",
    "rollup": "4.5.0",
    "jest": "29.7.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "@types/jest": "29.5.9",
    "@types/react": "18.2.38",
    "@types/react-dom": "^18",
    "@testing-library/jest-dom": "6.1.4"
  },
  "packageManager": "yarn@4.0.2",
  "workspaces": [
    "packages/**/**",
    "configs/*",
    "docs"
  ]
}
